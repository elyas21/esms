    "version": "0.2.0",
    "configurations": [
    
        {
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "name": "nodemon",
            "program": "${workspaceFolder}/server/bin/www",
            "request": "launch",
            "restart": true,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "runtimeExecutable": "/home/black/.nvm/versions/node/v14.17.3/bin/nodemon"
        },
        {
            "name": "seed2 Program",
            "comand": "nvm use default",
            "program": "${workspaceFolder}/server/seed/seed2/indexx.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "runtimeExecutable": "/home/black/.nvm/ /node/v14.17.3/bin/node"
        },
    
        {
            "name": "Launch Angular",
            "type": "edge",
            "request": "launch",
            "preLaunchTask": "npm: start",
            "url": "http://localhost:4200/",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch Angular",
            "type": "chrome",
            "request": "launch",
            "preLaunchTask": "npm: start",
            "url": "http://localhost:4200/",
            "webRoot": "${workspaceFolder}"
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/serve",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/dist/out-tsc/**/*.js",
                "${workspaceFolder}/node_modules/**",
                "${workspaceFolder}/server/node_modules/**",
                "${workspaceFolder}/src/**/*.ts",
                "${workspaceFolder}/server/try/**/*.js"
            ],
            "skipFiles": [
                "<node_internals>/**",
                "src/",
                "server/try/**",
                "src/**"
            ],
        
        }
    ]
}